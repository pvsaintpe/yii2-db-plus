{
  "name": "pvsaintpe/yii2-db-plus",
  "description": "Package",
  "keywords": [
    "yii2",
    "gii",
    "code generator"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Veselov",
      "email": "pvsaintpe@icloud.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "yiisoft/yii2": "2.0.15.1",
    "yiisoft/yii2-bootstrap": "2.0.6",
    "yiisoft/yii2-swiftmailer": "~2.0.0",
    "yiisoft/yii2-imagine": "2.0.3",
    "yiisoft/yii2-authclient": "^2.1",
    "yiisoft/yii2-debug": "2.0.6",
    "kartik-v/yii2-editable": "1.7.4",
    "kartik-v/yii2-datecontrol": "1.9.4",
    "kartik-v/yii2-date-range": "1.6.7",
    "kartik-v/yii2-field-range": "1.3.0",
    "kartik-v/yii2-tabs-x": "1.2.2",
    "kartik-v/yii2-widgets": "3.4.0",
    "kartik-v/yii2-icons": "1.4.1",
    "kartik-v/yii2-dropdown-x": "*",
    "kartik-v/yii2-export": "1.2.5",
    "kartik-v/yii2-grid": "3.2.5",
    "kartik-v/yii2-detail-view": "1.7.6",
    "pvsaintpe/yii2-boost": "*",
    "kartik-v/yii2-mpdf": "@dev"
  },
  "autoload": {
    "psr-4": {
      "pvsaintpe\\db\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
